{
  "extends": ["airbnb"],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/extensions": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".d.ts",
          ".ts"
        ]
      }
    }
  }
}
